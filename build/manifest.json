{
  "description": "NetHelper empowers quick access to NS records based on internal ids.",
  "manifest_version": 2,
  "name": "NetHelper",
  "version": "2.1.2",
  "browser_action": {
    "default_popup": "main.html",
    "default_icon": {
      "16": "images/NetHelper-Icon-16x16.png",
      "32": "images/NetHelper-Icon-32x32.png"
    }
  },
  "options_ui": {
    "page": "options.html"
  },
  "applications": {
    "gecko": {
      "id": "NetHelper@moracode.com"
    }
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}